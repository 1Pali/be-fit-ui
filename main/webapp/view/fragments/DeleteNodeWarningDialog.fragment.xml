<core:FragmentDefinition xmlns="sap.m"
	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified"
	xmlns:l="sap.ui.layout"
	xmlns:data="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog id="deleteNodeWarningDialog" title="{i18n>DELETE_NODE_TARGET_DIALOG_TITLE}"
		resizable="false" state="Warning">
		<content>
			<FlexBox direction="Column" height="auto" alignItems="Center"
				justifyContent="Center">
				<items>
					<Text text="{parts: [
						'i18n>DELETE_NODE_TARGET_WARNING',
						'selectedNode>/name'
						],
						formatter: 'jQuery.sap.formatMessage'}" class="sapUiMediumMargin" />
				</items>
			</FlexBox>
		</content>
		<buttons>
			<Button id="deleteNodeGoBtn" press="onDeleteNode" text="{i18n>OK_LABEL}" class="deleteNodeGoBtn"/>
			<Button press="onDeleteNodeWarningClosePress" text="{i18n>CANCEL}" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>